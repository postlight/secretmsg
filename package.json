    "unistore": "3.2.1"
  },
  "devDependencies": {
    "fastify": "1.14.1",
    "fastify-static": "1.1.0",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.11.0",
    "typescript": "3.3.3"
  }
}
