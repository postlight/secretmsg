  "main": "index.js",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:client": "parcel build src/client.ts",
    "build:worker": "parcel build src/worker.ts",
    "dev": "npm-run-all dev:server-bundle --parallel dev:client-bundle dev:server",
    "dev:server": "node server.js",
    "dev:server-bundle": "parcel build --no-minify -t node src/app.tsx",
    "dev:client-bundle": "parcel watch src/client.ts"
  },
